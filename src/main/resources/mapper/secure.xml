<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.forestfull.mapper.SecureMapper">
    <select id="getUserEntity" resultType="userEntity">
        SELECT
            *
        FROM
            user_entity ue
        WHERE
            ue.email = #{email}
    </select>

</mapper>